Procedure TMainForm.TТЕНЬTimer(Sender: TObject);
Begin
MainForm.ListBox3.Items.Add(Pr(Ранг)+'Таймер TТЕНЬ Сработал!');
SТЕНЬ:= True;
Есть_переключения := false;
Repeat
Переключения;
until not Есть_переключения;
Алгоритм;
SТЕНЬ:= False;
TТЕНЬ.Interval := 0;
TТЕНЬ.Enabled:= false;
Есть_переключения := false;
Repeat
Переключения;
until not Есть_переключения;
end;
